\n                <SendBtn onClick={sendNewAns} />\r\n            </div>\r\n            {errMsg && <SnackBar messageErr={errMsg} />}\r\n            {succMsg && <SnackBarSuccess message={succMsg} />}\r\n            <Snackbar />\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default CreateAnswer\r\n","C:\\html\\JS\\ReactJS\\resolver\\client\\src\\components\\InputAdd.jsx",[],"C:\\html\\JS\\ReactJS\\resolver\\client\\src\\components\\MemoProblem.jsx",[],"C:\\html\\JS\\ReactJS\\resolver\\client\\src\\components\\SnackBarSuccess.jsx",["163","164"],"import React from 'react';\r\nimport Button from '@material-ui/core/Button';\r\nimport Snackbar from '@material-ui/core/Snackbar';\r\nimport MuiAlert from '@material-ui/lab/Alert';\r\nimport { makeStyles } from '@material-ui/core/styles';\r\n\r\nfunction Alert(props) {\r\n    return <MuiAlert elevation={6} variant=\"filled\" {...props} />;\r\n}\r\n\r\nconst useStyles = makeStyles((theme) => ({\r\n    root: {\r\n        width: '100%',\r\n        '& > * + *': {\r\n            marginTop: theme.spacing(2),\r\n        },\r\n    },\r\n}));\r\n\r\nexport default function SnackBarSuccess({ message }) {\r\n    const classes = useStyles();\r\n    const [open, setOpen] = React.useState(true);\r\n\r\n    const handleClick = () => {\r\n        setOpen(true);\r\n    };\r\n\r\n    const handleClose = (event, reason) => {\r\n        if (reason === 'clickaway') {\r\n            return;\r\n        }\r\n\r\n        setOpen(false);\r\n    };\r\n\r\n    return (\r\n        <div className={classes.root}>\r\n            {/* <Button variant=\"outlined\" onClick={handleClick}>\r\n                Open success snackbar\r\n            </Button> */}\r\n            <Snackbar open={open} autoHideDuration={2000} onClose={handleClose}>\r\n                <Alert onClose={handleClose} severity=\"success\">\r\n                    {message}\r\n                </Alert>\r\n            </Snackbar>\r\n        </div>\r\n    );\r\n}\r\n","C:\\html\\JS\\ReactJS\\resolver